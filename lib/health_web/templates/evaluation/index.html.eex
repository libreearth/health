<div>
  <div id="map" class="full-map"></div>
</div>
<script>
  window.addEventListener('load', (event) => {
    window.init_map([<%= for {agg,index} <- Enum.with_index(@aggregations) do %> 
    <%= if index>0 do %>,<% end %>{hexagon: "<%= agg.hexagon %>", value: <%= agg.value %>} 
    <%end %>]);
  });
</script>
<style>
#map {
    width: 400px;
    height: 300px;
}
</style>

